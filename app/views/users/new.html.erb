<h1>Criar Conta</h1>

<%= form_for(@user) do |f| %>
	<%= f.text_field :first_name, :placeholder => "Nome" %> <br />
	<%= f.text_field :last_name, :placeholder => "Sobrenome" %> <br />
	<%= f.text_field :email, :placeholder => "Email" %> <br />
	<%= f.password_field :password, :placeholder => "Senha" %> <br />
	<%= f.submit "Criar" %>
<% end %>